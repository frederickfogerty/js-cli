{
	"compilerOptions": {
		"target": "es6",
		"outDir": "dist",
		"module": "commonjs",
		"noImplicitAny": true,
		"strictNullChecks": true,
		"removeComments": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"inlineSources": true,
		"pretty": true,
		"lib": [
			"es2015",
			"es2016"
		]
	}
}
