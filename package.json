{
	"name": "@fredfogerty/js-cli",
	"version": "0.0.5",
	"description": "An opinionated javascript command line tool",
	"main": "index.js",
	"scripts": {
		"build": "rm -rf dist && tsc && babel dist -d dist",
		"build:watch": "tsc -w",
		"lint": "tslint 'src/**/*.ts{,x}' --type-check --format verbose",
		"prepublish": "npm run build",
		"np": "ynp",
		"test": "npm run lint",
		"test:unit": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/frederickfogerty/ts-boilerplate.git"
	},
	"author": "Frederick Fogerty",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/frederickfogerty/ts-boilerplate/issues"
	},
	"homepage": "https://github.com/frederickfogerty/ts-boilerplate#readme",
	"jest": {
		"scriptPreprocessor": "<rootDir>/node_modules/ts-jest/preprocessor.js",
		"testPathDirs": [
			"src"
		],
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		]
	},
	"dependencies": {
		"@types/adm-zip": "^0.4.30",
		"@types/babel-core": "^6.7.14",
		"@types/bluebird": "^3.0.35",
		"@types/chai": "^3.4.34",
		"@types/chokidar": "^1.4.29",
		"@types/dotenv": "^2.0.19",
		"@types/express": "^4.0.34",
		"@types/filesize": "^3.2.15",
		"@types/fs-extra-promise": "^0.0.30",
		"@types/inquirer": "^0.0.31",
		"@types/invariant": "^2.2.28",
		"@types/node": "^6.0.49",
		"@types/node-fetch": "^1.6.5",
		"@types/pluralize": "^0.0.27",
		"@types/ramda": "^0.0.2",
		"@types/rsync": "^0.4.28",
		"adm-zip": "^0.4.7",
		"babel-cli": "^6.18.0",
		"babel-core": "^6.18.0",
		"babel-polyfill": "^6.16.0",
		"babel-preset-node6": "^11.0.0",
		"bluebird": "^3.4.6",
		"chokidar": "^1.6.1",
		"command-interface": "^2.4.0",
		"dotenv": "^2.0.0",
		"express": "^4.14.0",
		"filesize": "^3.3.0",
		"fs-extra-promise": "^0.4.1",
		"fuzzy": "^0.1.3",
		"inquirer": "^1.2.3",
		"invariant": "^2.2.2",
		"moment": "^2.16.0",
		"node-fetch": "^1.6.3",
		"pluralize": "^3.0.0",
		"ramda": "^0.22.1",
		"rsync": "^0.5.0",
		"rxjs": "5.0.0-rc.4",
		"toposort": "^1.0.0",
		"ts-babel-node": "^1.1.1",
		"ts-node": "^1.7.0",
		"typescript": "2.1"
	},
	"devDependencies": {
		"@types/jest": "^16.0.0",
		"jest": "^17.0.3",
		"ts-jest": "^17.0.0",
		"tslint": "^4.0.1",
		"tslint-config-xo": "^0.2.0",
		"ynp": "^2.9.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"bin": {
		"j": "index.js"
	}
}
