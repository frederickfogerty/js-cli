{
  "name": "@fredfogerty/js-cli",
  "version": "0.8.2",
  "description": "An opinionated javascript command line tool",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts{,x}' --type-check --format verbose --fix",
    "np": "ynp",
    "prepublish": "npm run build",
    "test": "npm run lint",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frederickfogerty/ts-boilerplate.git"
  },
  "author": "Frederick Fogerty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frederickfogerty/ts-boilerplate/issues"
  },
  "homepage": "https://github.com/frederickfogerty/ts-boilerplate#readme",
  "jest": {
    "testPathDirs": [
      "src"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@fredfogerty/tslint-config": "^3.0.2",
    "@types/adm-zip": "^0.4.30",
    "@types/babel-core": "^6.7.14",
    "@types/bluebird": "^3.0.35",
    "@types/chai": "^3.4.34",
    "@types/chokidar": "^1.4.29",
    "@types/debounce": "^0.0.30",
    "@types/dotenv": "^2.0.19",
    "@types/express": "^4.0.34",
    "@types/filesize": "^3.2.15",
    "@types/fs-extra-promise": "^0.0.30",
    "@types/inquirer": "^0.0.31",
    "@types/invariant": "^2.2.28",
    "@types/minimist": "^1.1.29",
    "@types/node": "^6.0.49",
    "@types/node-fetch": "^1.6.5",
    "@types/pluralize": "^0.0.27",
    "@types/ramda": "^0.0.2",
    "@types/rsync": "^0.4.28",
    "adm-zip": "^0.4.7",
    "any-observable": "^0.2.0",
    "bluebird": "^3.4.6",
    "chokidar": "^1.6.1",
    "command-interface": "^2.4.0",
    "debounce": "^1.0.0",
    "dotenv": "^2.0.0",
    "execa": "^0.5.0",
    "express": "^4.14.0",
    "filesize": "^3.3.0",
    "fs-extra-promise": "^0.4.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^1.2.3",
    "invariant": "^2.2.2",
    "listr": "^0.10.0",
    "minimist": "^1.2.0",
    "moment": "^2.16.0",
    "node-fetch": "^1.6.3",
    "observable-to-promise": "^0.4.0",
    "pluralize": "^3.0.0",
    "ramda": "^0.22.1",
    "rsync": "^0.5.0",
    "rxjs": "5.0.0-rc.4",
    "split": "^1.0.0",
    "stream-to-observable": "^0.2.0",
    "toposort": "^1.0.0",
    "ts-babel-node": "^1.1.1",
    "ts-node": "^1.7.0",
    "typescript": "^2.1.4",
    "updeep": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^16.0.0",
    "jest": "^17.0.3",
    "ts-jest": "^17.0.0",
    "tslint": "^4.2.0",
    "tslint-config-xo": "^0.2.0",
    "ynp": "^2.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "j": "index.js"
  }
}
